  <style>
    :root {
      --bg-color: #0ee9e5;
      --text-color: #1a1a2e;
      --border-color: #bdd7ee;
      --accent-color: #0ea5e9;
      --accent-hover: #0284c7;
      --card-bg: rgba(255, 255, 255, 0.9);
      --hero-gradient-start: #0ee9e5;
      --hero-gradient-end: #00507a;
      --glass-bg: rgba(240, 248, 255, 0.7);
      --glass-border: rgba(14, 165, 233, 0.3);
      --shadow-color: rgba(14, 165, 233, 0.15);
    }

    [data-theme="dark"] {
      --bg-color: #000000;
      --text-color: #3b82f6;
      --border-color: #1e293b;
      --accent-color: #3b82f6;
      --accent-hover: #60a5fa;
      --card-bg: rgba(10, 10, 26, 0.9);
      --hero-gradient-start: #0a0a0f;
      --hero-gradient-end: #1e293b;
      --glass-bg: rgba(10, 10, 20, 0.7);
      --glass-border: rgba(59, 130, 246, 0.3);
      --shadow-color: rgba(59, 130, 246, 0.2);
    }

    * {
      margin: 0;
      padding: 0;
      box-sizing: border-box;
    }

    html {
      scroll-behavior: smooth;
    }

    body {
      font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, 'Helvetica Neue', Arial, sans-serif;
      background-color: var(--bg-color);
      color: var(--text-color);
      line-height: 1.6;
      overflow-x: hidden;
    }

    /* Animated Background */
    .animated-bg {
      position: fixed;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      z-index: -1;
      background: linear-gradient(135deg, var(--hero-gradient-start) 0%, var(--hero-gradient-end) 100%);
      opacity: 0.05;
    }

    [data-theme="dark"] .animated-bg {
      opacity: 0.05;
    }

    .animated-bg::before {
      content: '';
      position: absolute;
      top: -50%;
      left: -50%;
      width: 200%;
      height: 200%;
      background: radial-gradient(circle, var(--accent-color) 1px, transparent 1px);
      background-size: 50px 50px;
      animation: bgMove 20s linear infinite;
      opacity: 0.3;
    }

    /* Dark Mode: Blaues animiertes Hintergrundmuster */
    [data-theme="dark"] .animated-bg::before {
      background: radial-gradient(circle, rgba(59, 130, 246, 0.4) 1px, transparent 1px);
    }

    @keyframes bgMove {
      0% { transform: translate(0, 0); }
      100% { transform: translate(50px, 50px); }
    }

    /* Sticky Header */
    .header {
      position: fixed;
      top: 0;
      left: 0;
      right: 0;
      z-index: 999;
      background: var(--glass-bg);
      backdrop-filter: blur(20px);
      -webkit-backdrop-filter: blur(20px);
      border-bottom: 1px solid var(--glass-border);
      padding: 1rem 2rem;
      transform: translateY(-100%);
      animation: slideDown 0.5s ease forwards;
    }

    @keyframes slideDown {
      to { transform: translateY(0); }
    }

    .header-content {
      max-width: 1400px;
      margin: 0 auto;
      display: flex;
      justify-content: space-between;
      align-items: center;
    }

    .logo {
      font-size: 1.5rem;
      font-weight: 700;
      color: var(--accent-color);
      text-decoration: none;
      display: flex;
      align-items: center;
      gap: 0.5rem;
    }

    .header-actions {
      display: flex;
      gap: 1rem;
      align-items: center;
      height: 2.5rem;

    }

    .header-actions > * {
      height: 2.5rem;
    }

    /* Language Selector Dropdown */
    .language-selector {
      position: relative;
      height: 2.5rem;
      display: flex;
      align-items: center;
    }

    .lang-current {
      background: var(--glass-bg);
      backdrop-filter: blur(10px);
      border: 2px solid var(--glass-border);
      border-radius: 0.5rem;
      padding: 0.5rem 0.8rem;
      cursor: pointer;
      transition: all 0.3s;
      display: flex;
      align-items: center;
      gap: 0.5rem;
      min-width: 80px;
      height: 2.5rem;
      box-sizing: border-box;
      flex-shrink: 0;
    }

    .lang-current:hover {
      border-color: var(--accent-color);
    }

    .lang-current img {
      width: 24px;
      height: 18px;
      border-radius: 2px;
    }

    .lang-current i {
      margin-left: auto;
      font-size: 0.8rem;
      transition: transform 0.3s;
    }

    .language-selector.open .lang-current i {
      transform: rotate(180deg);
    }

    .lang-dropdown {
      position: absolute;
      top: calc(100% + 0.5rem);
      right: 0;
      background: var(--glass-bg);
      backdrop-filter: blur(20px);
      border: 2px solid var(--glass-border);
      border-radius: 0.75rem;
      padding: 0.5rem;
      min-width: 200px;
      opacity: 0;
      visibility: hidden;
      transform: translateY(-10px);
      transition: all 0.3s;
      box-shadow: 0 10px 40px var(--shadow-color);
      z-index: 1000;
    }

    .language-selector.open .lang-dropdown {
      opacity: 1;
      visibility: visible;
      transform: translateY(0);
    }

    .lang-option {
      display: flex;
      align-items: center;
      gap: 0.75rem;
      padding: 0.6rem 0.75rem;
      border-radius: 0.5rem;
      text-decoration: none;
      color: var(--text-color);
      transition: all 0.2s;
    }

    .lang-option:hover {
      background: rgba(0, 184, 148, 0.1);
      transform: translateX(5px);
    }

    [data-theme="dark"] .lang-option:hover {
      background: rgba(0, 255, 136, 0.1);
    }

    .lang-option.active {
      background: rgba(0, 184, 148, 0.15);
      border-left: 3px solid var(--accent-color);
    }

    [data-theme="dark"] .lang-option.active {
      background: rgba(0, 255, 136, 0.15);
    }

    .lang-option img {
      width: 24px;
      height: 18px;
      border-radius: 2px;
    }

    .lang-option span {
      font-weight: 500;
    }

    /* Theme Toggle */
    .theme-toggle {
      background: var(--accent-color);
      color: #000;
      border: 0;
      padding: 0;
      margin: 0;
      width: 2.5rem;
      height: 2.5rem;
      border-radius: 50%;
      cursor: pointer;
      font-size: 1.2rem;
      display: flex;
      align-items: center;
      justify-content: center;
      transition: all 0.3s;
      box-sizing: border-box;
      flex-shrink: 0;
      line-height: 1;
    }

    .theme-toggle i {
      line-height: 1;
      display: flex;
      align-items: center;
      justify-content: center;
    }

    .theme-toggle:hover {
      transform: scale(1.1) rotate(20deg);
    }

    /* Dashboard Button */
    .btn-dashboard {
      background: var(--accent-color);
      color: #000;
      padding: 0 1.2rem;
      border-radius: 0.5rem;
      text-decoration: none;
      font-weight: 600;
      display: flex;
      align-items: center;
      gap: 0.5rem;
      transition: all 0.3s;
      font-size: 0.95rem;
      height: 2.5rem;
      box-sizing: border-box;
      flex-shrink: 0;
      box-shadow: 0 2px 8px rgba(0, 184, 148, 0.3);
    }

    .btn-dashboard:hover {
      background: var(--accent-hover);
      transform: translateY(-2px);
      box-shadow: 0 4px 12px rgba(0, 184, 148, 0.4);
    }

    [data-theme="dark"] .btn-dashboard {
      background: var(--accent-color);
      color: #000;
      box-shadow: 0 2px 8px rgba(0, 255, 136, 0.3);
    }

    [data-theme="dark"] .btn-dashboard:hover {
      background: var(--accent-hover);
      box-shadow: 0 4px 12px rgba(0, 255, 136, 0.4);
    }

    .btn-dashboard.loading {
      pointer-events: none;
      opacity: 0.7;
    }

    .btn-dashboard.loading i {
      animation: spin 1s linear infinite;
    }

    @keyframes spin {
      0% { transform: rotate(0deg); }
      100% { transform: rotate(360deg); }
    }

    /* Hero Section */
    .hero {
      min-height: 100vh;
      display: flex;
      align-items: center;
      justify-content: center;
      text-align: center;
      padding: 8rem 2rem 4rem;
      background: linear-gradient(135deg, var(--hero-gradient-start) 0%, var(--hero-gradient-end) 100%);
      position: relative;
      overflow: hidden;
    }

    .hero::before {
      content: '';
      position: absolute;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
      background: url('data:image/svg+xml,<svg width="100" height="100" xmlns="http://www.w3.org/2000/svg"><circle cx="50" cy="50" r="2" fill="white" opacity="0.1"/></svg>');
      animation: float 20s linear infinite;
    }

    @keyframes float {
      0% { transform: translateY(0); }
      100% { transform: translateY(-100px); }
    }

    .hero-content {
      position: relative;
      z-index: 1;
      max-width: 900px;
      animation: fadeInUp 0.8s ease;
    }

    @keyframes fadeInUp {
      from {
        opacity: 0;
        transform: translateY(30px);
      }
      to {
        opacity: 1;
        transform: translateY(0);
      }
    }

    .hero h1 {
      font-size: clamp(2.5rem, 5vw, 4rem);
      font-weight: 800;
      margin-bottom: 1rem;
      color: white;
      text-shadow: 0 2px 20px rgba(0, 0, 0, 0.3);
    }

    [data-theme="dark"] .hero h1 {
      color: var(--accent-color);
    }

    .hero-subtitle {
      font-size: clamp(1.1rem, 2vw, 1.5rem);
      color: rgba(255, 255, 255, 0.95);
      margin-bottom: 1rem;
      font-weight: 500;
    }

    [data-theme="dark"] .hero-subtitle {
      color: var(--text-color);
    }

    .version-badge {
      display: inline-block;
      background: rgba(255, 255, 255, 0.2);
      backdrop-filter: blur(10px);
      padding: 0.5rem 1.2rem;
      border-radius: 2rem;
      font-size: 0.9rem;
      margin-bottom: 2rem;
      border: 1px solid rgba(255, 255, 255, 0.3);
    }

    [data-theme="dark"] .version-badge {
      background: rgba(0, 255, 136, 0.2);
      border-color: rgba(0, 255, 136, 0.3);
      color: var(--accent-color);
    }

    .user-info {
      background: rgba(255, 255, 255, 0.15);
      backdrop-filter: blur(10px);
      padding: 0.8rem 1.5rem;
      border-radius: 2rem;
      margin-bottom: 2rem;
      display: inline-block;
      border: 1px solid rgba(255, 255, 255, 0.2);
    }

    [data-theme="dark"] .user-info {
      background: rgba(0, 255, 136, 0.15);
      border-color: rgba(0, 255, 136, 0.2);
      color: var(--text-color);
    }

    /* CTA Buttons */
    .cta-container {
      display: flex;
      gap: 1rem;
      justify-content: center;
      flex-wrap: wrap;
      margin-top: 2rem;
    }

    .btn-primary, .btn-secondary {
      padding: 1.2rem 2.5rem;
      font-size: 1.1rem;
      font-weight: 700;
      border-radius: 0.75rem;
      text-decoration: none;
      display: inline-flex;
      align-items: center;
      gap: 0.5rem;
      transition: all 0.3s;
      position: relative;
      overflow: hidden;
    }

    .btn-primary {
      background: white;
      color: var(--hero-gradient-start);
      border: 2px solid white;
      box-shadow: 0 10px 30px rgba(0, 0, 0, 0.2);
      animation: pulse 2s ease-in-out infinite;
    }

    @keyframes pulse {
      0%, 100% { box-shadow: 0 10px 30px rgba(0, 0, 0, 0.2); }
      50% { box-shadow: 0 10px 40px rgba(0, 0, 0, 0.3), 0 0 0 10px rgba(255, 255, 255, 0.1); }
    }

    .btn-primary:hover {
      transform: translateY(-3px);
      box-shadow: 0 15px 40px rgba(0, 0, 0, 0.3);
    }

    .btn-secondary {
      background: rgba(255, 255, 255, 0.2);
      color: white;
      border: 2px solid rgba(255, 255, 255, 0.5);
      backdrop-filter: blur(10px);
    }

    .btn-secondary:hover {
      background: rgba(255, 255, 255, 0.3);
      transform: translateY(-3px);
    }

    [data-theme="dark"] .btn-primary {
      background: var(--accent-color);
      color: #000;
      border-color: var(--accent-color);
    }

    [data-theme="dark"] .btn-secondary {
      background: rgba(0, 255, 136, 0.2);
      color: var(--accent-color);
      border-color: var(--accent-color);
    }

    /* Stats Section */
    .stats-section {
      padding: 4rem 2rem;
      background: var(--glass-bg);
      backdrop-filter: blur(20px);
      margin: 2rem auto;
      max-width: 1200px;
      border-radius: 2rem;
      border: 1px solid var(--glass-border);
    }

    .stats-grid {
      display: grid;
      grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
      gap: 2rem;
      text-align: center;
    }

    .stat-item {
      animation: fadeIn 1s ease;
    }

    .stat-number {
      font-size: 3rem;
      font-weight: 800;
      color: var(--accent-color);
      display: block;
      margin-bottom: 0.5rem;
    }

    .stat-label {
      font-size: 1rem;
      opacity: 0.8;
    }

    /* Features Section */
    .section {
      max-width: 1400px;
      margin: 6rem auto;
      padding: 0 2rem;
    }

    .section-title {
      text-align: center;
      font-size: clamp(2rem, 4vw, 3rem);
      margin-bottom: 1rem;
      color: var(--accent-color);
    }

    .section-subtitle {
      text-align: center;
      font-size: 1.2rem;
      opacity: 0.8;
      margin-bottom: 4rem;
      max-width: 700px;
      margin-left: auto;
      margin-right: auto;
    }

    .features-grid {
      display: grid;
      grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
      gap: 2rem;
    }

    .feature-card {
      background: var(--glass-bg);
      backdrop-filter: blur(20px);
      border: 1px solid var(--glass-border);
      border-radius: 1.5rem;
      padding: 2.5rem;
      transition: all 0.3s;
      opacity: 0;
      transform: translateY(30px);
    }

    .feature-card.visible {
      animation: slideUp 0.6s ease forwards;
    }

    @keyframes slideUp {
      to {
        opacity: 1;
        transform: translateY(0);
      }
    }

    .feature-card:hover {
      transform: translateY(-10px);
      box-shadow: 0 20px 50px var(--shadow-color);
      border-color: var(--accent-color);
    }

    .feature-icon {
      font-size: 3rem;
      color: var(--accent-color);
      margin-bottom: 1.5rem;
      display: block;
    }

    .feature-card h3 {
      font-size: 1.5rem;
      margin-bottom: 1rem;
      color: var(--accent-color);
    }

    .feature-card p {
      opacity: 0.9;
      line-height: 1.8;
    }

    /* Pricing Section */
    .pricing-grid {
      display: grid;
      grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
      gap: 2rem;
      margin-top: 3rem;
    }

    .pricing-card {
      background: var(--glass-bg);
      backdrop-filter: blur(20px);
      border: 2px solid var(--glass-border);
      border-radius: 1.5rem;
      padding: 2.5rem;
      text-align: center;
      transition: all 0.3s;
      position: relative;
    }

    .pricing-card.featured {
      border-color: var(--accent-color);
      box-shadow: 0 0 50px rgba(0, 184, 148, 0.3);
      transform: scale(1.05);
    }

    [data-theme="dark"] .pricing-card.featured {
      box-shadow: 0 0 50px rgba(0, 255, 136, 0.3);
    }

    .pricing-badge {
      position: absolute;
      top: -15px;
      left: 50%;
      transform: translateX(-50%);
      background: var(--accent-color);
      color: white;
      padding: 0.5rem 1.5rem;
      border-radius: 2rem;
      font-size: 0.85rem;
      font-weight: 700;
      text-transform: uppercase;
    }

    [data-theme="dark"] .pricing-badge {
      color: #000;
    }

    .pricing-card h3 {
      font-size: 1.8rem;
      margin-bottom: 1rem;
      color: var(--accent-color);
    }

    .pricing-price {
      font-size: 3rem;
      font-weight: 800;
      color: var(--accent-color);
      margin: 1rem 0;
    }

    .pricing-price span {
      font-size: 1.2rem;
      opacity: 0.7;
    }

    .pricing-features {
      list-style: none;
      margin: 2rem 0;
      text-align: left;
    }

    .pricing-features li {
      padding: 0.8rem 0;
      border-bottom: 1px solid var(--border-color);
      display: flex;
      align-items: center;
      gap: 0.5rem;
    }

    .pricing-features li:last-child {
      border-bottom: none;
    }

    .pricing-features i {
      color: var(--accent-color);
    }

    /* Testimonials Section */
    .testimonials-grid {
      display: grid;
      grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
      gap: 2rem;
      margin-top: 3rem;
    }

    .testimonial-card {
      background: var(--glass-bg);
      backdrop-filter: blur(20px);
      border: 1px solid var(--glass-border);
      border-radius: 1.5rem;
      padding: 2rem;
      transition: all 0.3s;
    }

    .testimonial-card:hover {
      transform: translateY(-5px);
      box-shadow: 0 10px 30px var(--shadow-color);
    }

    .testimonial-rating {
      color: #ffc107;
      font-size: 1.2rem;
      margin-bottom: 1rem;
    }

    .testimonial-text {
      font-style: italic;
      margin-bottom: 1.5rem;
      line-height: 1.8;
      opacity: 0.9;
    }

    .testimonial-author {
      display: flex;
      align-items: center;
      gap: 1rem;
    }

    .testimonial-avatar {
      width: 50px;
      height: 50px;
      border-radius: 50%;
      background: var(--accent-color);
      display: flex;
      align-items: center;
      justify-content: center;
      font-size: 1.5rem;
      color: white;
      font-weight: 700;
      overflow: hidden;
      flex-shrink: 0;
    }

    .testimonial-avatar img {
      width: 100%;
      height: 100%;
      object-fit: cover;
      border-radius: 50%;
    }

    .testimonial-info h4 {
      margin-bottom: 0.2rem;
      color: var(--accent-color);
    }

    .testimonial-info p {
      opacity: 0.7;
      font-size: 0.9rem;
    }

    /* Partners Section */
    .partners-grid {
      display: grid;
      grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
      gap: 2rem;
      margin-top: 3rem;
    }

    .partner-card {
      background: var(--glass-bg);
      backdrop-filter: blur(20px);
      border: 1px solid var(--glass-border);
      border-radius: 1.5rem;
      padding: 2rem;
      transition: all 0.3s;
      text-align: center;
      text-decoration: none;
      color: inherit;
      display: block;
    }

    .partner-card:hover {
      transform: translateY(-5px);
      box-shadow: 0 10px 30px var(--shadow-color);
      border-color: var(--accent-color);
    }

    .partner-icon {
      width: 80px;
      height: 80px;
      border-radius: 50%;
      margin: 0 auto 1.5rem;
      overflow: hidden;
      border: 3px solid var(--accent-color);
      background: var(--glass-bg);
      display: flex;
      align-items: center;
      justify-content: center;
      font-size: 2rem;
      font-weight: 700;
      color: var(--accent-color);
    }

    .partner-icon img {
      width: 100%;
      height: 100%;
      object-fit: cover;
    }

    .partner-name {
      font-size: 1.3rem;
      font-weight: 700;
      margin-bottom: 0.5rem;
      color: var(--accent-color);
    }

    .partner-badge {
      display: inline-block;
      background: linear-gradient(135deg, var(--accent-color), #00cc99);
      color: white;
      padding: 0.4rem 1rem;
      border-radius: 2rem;
      font-size: 0.85rem;
      font-weight: 600;
      margin-top: 0.5rem;
    }

    /* FAQ Section */
    .faq-container {
      max-width: 800px;
      margin: 3rem auto;
    }

    .faq-item {
      background: var(--glass-bg);
      backdrop-filter: blur(20px);
      border: 1px solid var(--glass-border);
      border-radius: 1rem;
      margin-bottom: 1rem;
      overflow: hidden;
      transition: all 0.3s;
    }

    .faq-question {
      padding: 1.5rem;
      cursor: pointer;
      display: flex;
      justify-content: space-between;
      align-items: center;
      font-weight: 600;
      font-size: 1.1rem;
    }

    .faq-question:hover {
      background: rgba(0, 184, 148, 0.05);
    }

    [data-theme="dark"] .faq-question:hover {
      background: rgba(0, 255, 136, 0.05);
    }

    .faq-icon {
      color: var(--accent-color);
      transition: transform 0.3s;
    }

    .faq-item.active .faq-icon {
      transform: rotate(180deg);
    }

    .faq-answer {
      max-height: 0;
      overflow: hidden;
      transition: max-height 0.3s ease;
      padding: 0 1.5rem;
    }

    .faq-item.active .faq-answer {
      max-height: 500px;
      padding: 0 1.5rem 1.5rem;
    }

    .faq-answer p {
      opacity: 0.9;
      line-height: 1.8;
    }

    /* Footer */
    .footer {
      background: var(--glass-bg);
      backdrop-filter: blur(20px);
      border-top: 1px solid var(--glass-border);
      padding: 3rem 2rem;
      margin-top: 6rem;
    }

    .footer-content {
      max-width: 1400px;
      margin: 0 auto;
      display: grid;
      grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
      gap: 3rem;
    }

    .footer-section h3 {
      color: var(--accent-color);
      margin-bottom: 1.5rem;
      font-size: 1.3rem;
    }

    .footer-links {
      list-style: none;
    }

    .footer-links li {
      margin-bottom: 0.8rem;
    }

    .footer-links a {
      color: var(--text-color);
      text-decoration: none;
      opacity: 0.8;
      transition: all 0.3s;
      display: flex;
      align-items: center;
      gap: 0.5rem;
    }

    .footer-links a:hover {
      opacity: 1;
      color: var(--accent-color);
      transform: translateX(5px);
    }

    .footer-bottom {
      text-align: center;
      margin-top: 3rem;
      padding-top: 2rem;
      border-top: 1px solid var(--border-color);
      opacity: 0.7;
    }

    /* Loading Overlay */
    .loading-overlay {
      position: fixed;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background: rgba(0, 0, 0, 0.95);
      display: none;
      justify-content: center;
      align-items: center;
      z-index: 99999;
    }

    .loading-overlay.show {
      display: flex;
    }

    .loading-content {
      text-align: center;
      background: rgba(255, 255, 255, 0.05);
      padding: 3rem;
      border-radius: 2rem;
      border: 1px solid rgba(0, 255, 136, 0.2);
    }

    .loading-spinner {
      width: 60px;
      height: 60px;
      border: 6px solid rgba(0, 255, 136, 0.1);
      border-top: 6px solid var(--accent-color);
      border-radius: 50%;
      animation: spin 1s linear infinite;
      margin: 0 auto 2rem;
    }

    @keyframes spin {
      0% { transform: rotate(0deg); }
      100% { transform: rotate(360deg); }
    }

    .loading-text {
      font-size: 1.3rem;
      color: var(--accent-color);
      font-weight: 600;
    }

    /* Mobile Responsive */
    @media (max-width: 768px) {
      .header {
        padding: 0.8rem 1rem;
      }

      .header-actions {
        gap: 0.5rem;
      }

      .lang-current {
        padding: 0.3rem 0.6rem;
      }

      .lang-current img {
        width: 20px;
        height: 15px;
      }

      .lang-dropdown {
        right: auto;
        left: 0;
      }

      .hero {
        padding: 6rem 1.5rem 3rem;
        min-height: auto;
      }

      .cta-container {
        flex-direction: column;
        width: 100%;
      }

      .btn-primary, .btn-secondary {
        width: 100%;
        justify-content: center;
      }

      .stats-section {
        margin: 1rem;
        padding: 2rem 1rem;
        border-radius: 1rem;
      }

      .stats-grid {
        grid-template-columns: repeat(2, 1fr);
        gap: 1.5rem;
      }

      .stat-number {
        font-size: 2rem;
      }

      .section {
        margin: 3rem auto;
        padding: 0 1rem;
      }

      .features-grid,
      .pricing-grid,
      .testimonials-grid {
        grid-template-columns: 1fr;
      }

      .pricing-card.featured {
        transform: scale(1);
      }

      .footer-content {
        grid-template-columns: 1fr;
        gap: 2rem;
      }
    }

    /* Scroll animations */
    @keyframes fadeIn {
      from {
        opacity: 0;
        transform: translateY(20px);
      }
      to {
        opacity: 1;
        transform: translateY(0);
      }
    }
  </style>
</head>
