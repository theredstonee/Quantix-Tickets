          }
        };

        showLoading(texts[href]?.[lang] || texts[href]?.de || 'Wird geladen...');

        setTimeout(() => {
          window.location.href = href;
        }, 100);
      });
    });

    // Lazy load images
    if ('loading' in HTMLImageElement.prototype) {
      const images = document.querySelectorAll('img[loading="lazy"]');
      images.forEach(img => {
        img.src = img.src;
      });
    }
  </script>
