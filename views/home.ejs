    <script>
      // Stats Counter Animation
      function animateCounter(el) {
        const targetStr = el.getAttribute('data-target');
        const target = parseFloat(targetStr);
        const duration = 2000;
        const step = target / (duration / 16);
        let current = 0;

        const timer = setInterval(() => {
          current += step;
          if (current >= target) {
            el.textContent = Math.floor(target);
            clearInterval(timer);
          } else {
            el.textContent = Math.floor(current);
          }
        }, 16);
      }

      // Animate all stat numbers
      document.querySelectorAll('.stat-number[data-target]').forEach(el => {
        animateCounter(el);
      });

      // Load real uptime
      async function loadUptime() {
        try {
          const response = await fetch('/api/uptime');
          if (!response.ok) throw new Error('HTTP error');
          
          const data = await response.json();
          if (data.success && data.uptime) {
            const uptimePercent = parseFloat(data.uptime.day30) || 99.5;
            document.getElementById('uptime-display').textContent = uptimePercent.toFixed(1);
          } else {
            document.getElementById('uptime-display').textContent = '99.5';
          }
        } catch (err) {
          console.error('Failed to load uptime:', err);
          document.getElementById('uptime-display').textContent = '99.5';
        }
      }

      // Load uptime on page load
      loadUptime();

      // Update uptime every 30 seconds
      setInterval(loadUptime, 30000);
    </script>
