  <script>
    // Language Dropdown Toggle
    function toggleLanguageDropdown() {
      const selector = document.getElementById('langSelector');
      selector.classList.toggle('open');
    }

    // Close dropdown when clicking outside
    document.addEventListener('click', function(event) {
      const selector = document.getElementById('langSelector');
      const isClickInside = selector.contains(event.target);
      if (!isClickInside && selector.classList.contains('open')) {
        selector.classList.remove('open');
      }
    });

    // Theme is managed by /js/theme-manager.js (Cookie-based)

    // Dashboard Button Loading Animation
    const dashboardBtn = document.getElementById('dashboardBtn');
    if (dashboardBtn) {
      dashboardBtn.addEventListener('click', function(e) {
        e.preventDefault();
        const href = this.getAttribute('href');
        this.classList.add('loading');
        setTimeout(() => {
          window.location.href = href;
        }, 300);
      });
    }

    // Stats Counter Animation
    function animateCounter(el) {
      const targetStr = el.getAttribute('data-target');
      const target = parseFloat(targetStr);
      const hasDecimal = targetStr.includes('.');
      const duration = 2000;
      const step = target / (duration / 16);
      let current = 0;

      const timer = setInterval(() => {
        current += step;
        if (current >= target) {
          el.textContent = hasDecimal ? target.toFixed(1) : target;
          clearInterval(timer);
        } else {
          el.textContent = hasDecimal ? current.toFixed(1) : Math.floor(current);
        }
      }, 16);
    }

    // Intersection Observer for scroll animations
    const observerOptions = {
      threshold: 0.1,
      rootMargin: '0px 0px -100px 0px'
    };

    const observer = new IntersectionObserver((entries) => {
      entries.forEach(entry => {
        if (entry.isIntersecting) {
          entry.target.classList.add('visible');

          // Animate stats when visible
          if (entry.target.classList.contains('stat-number')) {
            animateCounter(entry.target);
          }
        }
      });
    }, observerOptions);

    // Observe all feature cards and stats
    document.querySelectorAll('.feature-card, .stat-number, .pricing-card, .testimonial-card').forEach(el => {
      observer.observe(el);
    });

    // FAQ Toggle
    function toggleFaq(element) {
      const faqItem = element.parentElement;
      const wasActive = faqItem.classList.contains('active');

      // Close all FAQs
      document.querySelectorAll('.faq-item').forEach(item => {
        item.classList.remove('active');
      });

      // Open clicked FAQ if it wasn't active
      if (!wasActive) {
        faqItem.classList.add('active');
      }
    }

    // Load real uptime from UptimeRobot API and display as percentage
    async function loadUptime() {
      try {
        console.log('[Uptime] Fetching uptime data...');
        const response = await fetch('/api/uptime');

        if (!response.ok) {
          console.error('[Uptime] HTTP error:', response.status, response.statusText);
          throw new Error(`HTTP ${response.status}`);
        }

        const data = await response.json();
        console.log('[Uptime] Response data:', data);
        console.log('[Uptime] data.success:', data.success);
        console.log('[Uptime] data.uptime:', data.uptime);
        console.log('[Uptime] data.status:', data.status);

        if (data.success && data.uptime) {
          // Use real 30-day uptime from UptimeRobot
          const uptimePercent = parseFloat(data.uptime.day30) || 0;

          console.log('[Uptime] Raw day30 value:', data.uptime.day30);
          console.log('[Uptime] Parsed uptimePercent:', uptimePercent);

          // Display with 1 decimal place
          const displayValue = uptimePercent.toFixed(1);
          document.getElementById('uptime-display').textContent = displayValue;
          console.log('[Uptime] ✅ Displayed:', displayValue + '%');

          // Update status indicator if exists
          const statusEl = document.getElementById('uptime-status');
          if (statusEl) {
            if (data.status === 'online') {
              statusEl.textContent = '🟢 Online';
              statusEl.style.color = 'var(--accent-color)';
            } else if (data.status === 'down') {
              statusEl.textContent = '🔴 Down';
              statusEl.style.color = '#e74c3c';
            } else {
              statusEl.textContent = '🟡 Unknown';
              statusEl.style.color = '#f39c12';
            }
          }
        } else {
          // Fallback to default if UptimeRobot is not configured
          console.warn('[Uptime] ⚠️ UptimeRobot not configured or error:', data.error || 'No success flag');
          document.getElementById('uptime-display').textContent = '99.5';

          const statusEl = document.getElementById('uptime-status');
          if (statusEl) {
            statusEl.textContent = '⚙️ Nicht konfiguriert';
            statusEl.style.color = '#95a5a6';
          }
        }
      } catch (err) {
        console.error('[Uptime] ❌ Failed to load uptime:', err);
        // Fallback to default value
        document.getElementById('uptime-display').textContent = '99.5';

        const statusEl = document.getElementById('uptime-status');
        if (statusEl) {
          statusEl.textContent = '⚠️ Fehler';
          statusEl.style.color = '#e74c3c';
        }
      }
    }

    // Load uptime on page load
    loadUptime();

    // Update uptime every 30 seconds
    setInterval(loadUptime, 30000);

    // Loading overlay
    function showLoading(text) {
      const overlay = document.getElementById('loadingOverlay');
      const loadingText = document.getElementById('loadingText');
      loadingText.textContent = text;
      overlay.classList.add('show');
    }

    // Add loading to CTA buttons
    document.querySelectorAll('a[href="/login"], a[href="/select-server"], a[href="/logout"]').forEach(btn => {
      btn.addEventListener('click', function(e) {
        e.preventDefault();
