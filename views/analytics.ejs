<!doctype html>
<html lang="de">
<head>
  <!-- Theme Manager - Load before anything else -->
  <script src="/js/theme-manager.js"></script>
  <!-- Google tag (gtag.js) -->
  <script async src="https://www.googletagmanager.com/gtag/js?id=G-SLMX16ZJ6Y"></script>
  <script>
    window.dataLayer = window.dataLayer || [];
    function gtag(){dataLayer.push(arguments);}
    gtag('js', new Date());

    gtag('config', 'G-SLMX16ZJ6Y');
  </script>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Analytics - Quantix Tickets Bot</title>
  <!-- Favicon -->
  <link rel="icon" type="image/png" sizes="32x32" href="/images/favicon.png">
  <link rel="icon" type="image/png" sizes="16x16" href="/images/favicon.png">
  <link rel="apple-touch-icon" sizes="180x180" href="/images/favicon.png">
  <link rel="shortcut icon" href="/images/favicon.png">
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@picocss/pico@1/css/pico.min.css">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css">
  <style>
    :root {
      --bg-color: #f0f8ff;
      --text-color: #1a1a2e;
      --border-color: #bdd7ee;
      --accent-color: #0ea5e9;
      --accent-hover: #0284c7;
      --card-bg: rgba(255, 255, 255, 0.9);
      --success-color: #0ea5e9;
      --warning-color: #ff9900;
      --error-color: #ff4444;
      --glass-bg: rgba(240, 248, 255, 0.7);
      --glass-border: rgba(14, 165, 233, 0.3);
      --shadow-color: rgba(14, 165, 233, 0.15);
    }

    [data-theme="dark"] {
      --bg-color: #0a0a0a;
      --text-color: #00ff88;
      --border-color: #1a3a2a;
      --accent-color: #00ff88;
      --accent-hover: #00dd77;
      --card-bg: rgba(15, 21, 16, 0.9);
      --success-color: #00ff88;
      --warning-color: #ff9900;
      --error-color: #ff4444;
      --glass-bg: rgba(15, 21, 16, 0.7);
      --glass-border: rgba(0, 255, 136, 0.3);
      --shadow-color: rgba(0, 255, 136, 0.2);
    }

    [data-theme="dark"] body {
      background-color: var(--bg-color);
      color: var(--text-color);
    }

    /* Animated Background */
    body::before {
      content: '';
      position: fixed;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      z-index: -1;
      background: radial-gradient(circle, var(--accent-color) 1px, transparent 1px);
      background-size: 50px 50px;
      opacity: 0.03;
      animation: bgMove 20s linear infinite;
    }

    @keyframes bgMove {
      0% { transform: translate(0, 0); }
      100% { transform: translate(50px, 50px); }
    }

    body {
      padding: 2rem;
      min-height: 100vh;
    }

    .container {
      max-width: 1400px;
      margin: 0 auto;
    }

    .header {
      display: flex;
      justify-content: space-between;
      align-items: center;
      margin-bottom: 2rem;
      flex-wrap: wrap;
      gap: 1rem;
    }

    .stat-grid {
      display: grid;
      grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
      gap: 1.5rem;
      margin-bottom: 2rem;
    }

    .stat-card {
      background: var(--glass-bg);
      backdrop-filter: blur(10px);
      -webkit-backdrop-filter: blur(10px);
      border: 2px solid var(--glass-border);
      border-radius: 12px;
      padding: 1.5rem;
      transition: all 0.3s;
      box-shadow: 0 8px 32px var(--shadow-color);
    }

    .stat-card:hover {
      transform: translateY(-5px);
      box-shadow: 0 12px 40px var(--shadow-color);
      border-color: var(--accent-color);
    }

    .stat-card h3 {
      font-size: 0.9rem;
      text-transform: uppercase;
      letter-spacing: 0.5px;
      opacity: 0.7;
      margin-bottom: 0.5rem;
    }

    .stat-value {
      font-size: 2.5rem;
      font-weight: 700;
      color: var(--accent-color);
      margin-bottom: 0.5rem;
    }

    .stat-icon {
      font-size: 2rem;
      opacity: 0.5;
      float: right;
    }

    .chart-container {
      background: var(--glass-bg);
      backdrop-filter: blur(10px);
      -webkit-backdrop-filter: blur(10px);
      border: 2px solid var(--glass-border);
      border-radius: 12px;
      padding: 2rem;
      margin-bottom: 2rem;
      box-shadow: 0 8px 32px var(--shadow-color);
      transition: all 0.3s ease;
    }

    .chart-container:hover {
      transform: translateY(-2px);
      box-shadow: 0 12px 40px var(--shadow-color);
    }

    .chart-title {
      font-size: 1.3rem;
      font-weight: 600;
      margin-bottom: 1.5rem;
      color: var(--text-color);
    }

    .bar-chart {
      display: flex;
      flex-direction: column;
      gap: 1rem;
    }

    .bar-item {
      display: flex;
      align-items: center;
      gap: 1rem;
    }

    .bar-label {
      min-width: 150px;
      font-weight: 500;
    }

    .bar-container {
      flex: 1;
      height: 30px;
      background: rgba(0, 184, 148, 0.1);
      border-radius: 6px;
      overflow: hidden;
      position: relative;
    }

    .bar-fill {
      height: 100%;
      background: linear-gradient(90deg, var(--accent-color), var(--accent-hover));
      transition: width 0.5s ease;
      display: flex;
      align-items: center;
      justify-content: flex-end;
      padding-right: 10px;
      color: white;
      font-weight: 600;
      font-size: 0.85rem;
    }

    .back-btn {
      display: inline-flex;
      align-items: center;
      gap: 0.5rem;
      padding: 0.75rem 1.5rem;
      background: var(--accent-color);
      color: #000;
      text-decoration: none;
      border-radius: 8px;
      font-weight: 600;
      transition: all 0.2s;
    }

    .back-btn:hover {
      background: var(--accent-hover);
      transform: translateY(-2px);
    }

    .theme-toggle {
      position: fixed;
      top: 1.5rem;
      right: 1.5rem;
      background: var(--accent-color);
      color: #000;
      border: 0;
      width: 3rem;
      height: 3rem;
      border-radius: 50%;
      cursor: pointer;
      font-size: 1.5rem;
      display: flex;
      align-items: center;
      justify-content: center;
      transition: all 0.3s;
      box-shadow: 0 2px 8px rgba(0,0,0,0.2);
      z-index: 1000;
    }

    .theme-toggle:hover {
      transform: scale(1.1);
    }

    .table-container {
      overflow-x: auto;
    }

    table {
      width: 100%;
      border-collapse: collapse;
    }

    th, td {
      padding: 1rem;
      text-align: left;
      border-bottom: 1px solid var(--border-color);
    }

    th {
      background: var(--card-bg);
      font-weight: 600;
      text-transform: uppercase;
      font-size: 0.85rem;
      letter-spacing: 0.5px;
    }

    .badge {
      display: inline-block;
      padding: 0.25rem 0.75rem;
      border-radius: 20px;
      font-size: 0.85rem;
      font-weight: 600;
    }

    .badge-success {
      background: rgba(0, 255, 136, 0.2);
      color: var(--success-color);
    }

    .badge-warning {
      background: rgba(255, 153, 0, 0.2);
      color: var(--warning-color);
    }

    .badge-error {
      background: rgba(255, 68, 68, 0.2);
      color: var(--error-color);
    }

    @media (max-width: 768px) {
      body {
        padding: 1rem;
      }

      .stat-grid {
        grid-template-columns: 1fr;
      }

      .header {
        flex-direction: column;
        align-items: flex-start;
      }

      .bar-label {
        min-width: 100px;
        font-size: 0.85rem;
      }
    }

    /* Loading Overlay */
    .loading-overlay {
      position: fixed;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background: rgba(0,0,0,0.95);
      display: none;
      justify-content: center;
      align-items: center;
      z-index: 99999;
      flex-direction: column;
      gap: 2rem;
    }

    .loading-overlay.show {
      display: flex;
    }

    .loading-content {
      text-align: center;
      background: rgba(255,255,255,0.05);
      padding: 3rem;
      border-radius: 20px;
      border: 1px solid rgba(0,255,136,0.2);
      max-width: 400px;
      width: 90%;
    }

    .loading-spinner {
      width: 60px;
      height: 60px;
      border: 6px solid rgba(0,255,136,0.1);
      border-top: 6px solid var(--accent-color);
      border-radius: 50%;
      animation: loadingSpin 1s linear infinite;
      margin: 0 auto 2rem;
    }

    .loading-text {
      font-size: 1.3rem;
      color: var(--accent-color);
      font-weight: 600;
      margin-bottom: 0.5rem;
    }

    .loading-subtext {
      font-size: 0.9rem;
      opacity: 0.7;
      color: var(--text-color);
    }

    @keyframes loadingSpin {
      0% { transform: rotate(0deg); }
      100% { transform: rotate(360deg); }
    }
  </style>
</head>
<body>
  <!-- Loading Overlay -->
  <div class="loading-overlay" id="loadingOverlay">
    <div class="loading-content">
      <div class="loading-spinner"></div>
      <div class="loading-text" id="loadingText">Wird geladen...</div>
      <div class="loading-subtext">Bitte warten</div>
    </div>
  </div>

  <button class="theme-toggle" id="themeToggle" title="Dark Mode umschalten">
    <i class="fas fa-moon"></i>
  </button>

  <div class="container">
    <div class="header">
      <div>
        <h1><i class="fas fa-chart-line"></i> üìà Analytics</h1>
        <p style="opacity: 0.7; margin-top: 0.5rem;"><%= guildName %> ‚Ä¢ <%= stats.total %> Tickets gesamt</p>
      </div>
      <div style="display: flex; gap: 0.75rem; flex-wrap: wrap;">
        <a href="/panel" class="back-btn">
          <i class="fas fa-arrow-left"></i>
          Zur√ºck zum Panel
        </a>
        <a href="https://discord.com/invite/mnYbnpyyBS" target="_blank" class="back-btn" style="background: #5865F2; border-color: rgba(88, 101, 242, 0.3);">
          <i class="fab fa-discord"></i>
          Support Server
        </a>
        <% if (canExportCSV) { %>
        <a href="/export/tickets/csv" class="back-btn" style="background: linear-gradient(135deg, #10b981, #059669); border-color: rgba(16, 185, 129, 0.3);" title="Alle Tickets als CSV exportieren">
          <i class="fas fa-file-csv"></i>
          Export Tickets (CSV)
        </a>
        <a href="/export/stats/csv" class="back-btn" style="background: linear-gradient(135deg, #6366f1, #4f46e5); border-color: rgba(99, 102, 241, 0.3);" title="Statistiken als CSV exportieren">
          <i class="fas fa-chart-bar"></i>
          Export Stats (CSV)
        </a>
        <% } %>
      </div>
    </div>

    <!-- Overview Stats -->
    <div class="stat-grid">
      <div class="stat-card">
        <span class="stat-icon">üé´</span>
        <h3>Gesamt Tickets</h3>
        <div class="stat-value"><%= stats.total %></div>
      </div>

      <div class="stat-card">
        <span class="stat-icon">‚úÖ</span>
        <h3>Geschlossene</h3>
        <div class="stat-value"><%= stats.closed %></div>
        <small style="opacity: 0.7;"><%= stats.total > 0 ? Math.round((stats.closed / stats.total) * 100) : 0 %>%</small>
      </div>

      <div class="stat-card">
        <span class="stat-icon">üîì</span>
        <h3>Offene</h3>
        <div class="stat-value"><%= stats.open %></div>
        <small style="opacity: 0.7;"><%= stats.total > 0 ? Math.round((stats.open / stats.total) * 100) : 0 %>%</small>
      </div>

      <div class="stat-card">
        <span class="stat-icon">üë•</span>
        <h3>Geclaimt</h3>
        <div class="stat-value"><%= stats.claimed %></div>
        <small style="opacity: 0.7;"><%= stats.total > 0 ? Math.round((stats.claimed / stats.total) * 100) : 0 %>%</small>
      </div>
    </div>

    <!-- Tickets by Topic -->
    <% if (Object.keys(stats.byTopic).length > 0) { %>
    <div class="chart-container">
      <h2 class="chart-title">üìÅ Tickets nach Kategorie</h2>
      <div class="bar-chart">
        <%
        const topicEntries = Object.entries(stats.byTopic).sort((a, b) => b[1] - a[1]);
        const maxTopicCount = topicEntries[0]?.[1] || 1;
        topicEntries.forEach(([topic, count]) => {
          const percentage = (count / maxTopicCount) * 100;
        %>
        <div class="bar-item">
          <div class="bar-label"><%= topic %></div>
          <div class="bar-container">
            <div class="bar-fill" style="width: <%= percentage %>%"><%= count %></div>
          </div>
        </div>
        <% }) %>
      </div>
    </div>
    <% } %>

    <!-- Tickets by Priority -->
    <div class="chart-container">
      <h2 class="chart-title">üéØ Tickets nach Priorit√§t</h2>
      <div class="bar-chart">
        <%
        const priorities = [
          { label: 'üü¢ Gr√ºn (Niedrig)', value: stats.byPriority['0'] || 0, color: '#2bd94a' },
          { label: 'üü† Orange (Mittel)', value: stats.byPriority['1'] || 0, color: '#ff9900' },
          { label: 'üî¥ Rot (Hoch)', value: stats.byPriority['2'] || 0, color: '#d92b2b' }
        ];
        const maxPriorityCount = Math.max(...priorities.map(p => p.value)) || 1;
        priorities.forEach(p => {
          const percentage = (p.value / maxPriorityCount) * 100;
        %>
        <div class="bar-item">
          <div class="bar-label"><%= p.label %></div>
          <div class="bar-container">
            <div class="bar-fill" style="width: <%= percentage %>%; background: linear-gradient(90deg, <%= p.color %>, <%= p.color %>dd);"><%= p.value %></div>
          </div>
        </div>
        <% }) %>
      </div>
    </div>

    <!-- Top Claimer -->
    <% if (stats.topClaimers && stats.topClaimers.length > 0) { %>
    <div class="chart-container">
      <h2 class="chart-title">üëë Top Ticket-Bearbeiter</h2>
      <div class="table-container">
        <table>
          <thead>
            <tr>
              <th>Rang</th>
              <th>Bearbeiter</th>
              <th>Tickets</th>
              <th>Anteil</th>
            </tr>
          </thead>
          <tbody>
            <% stats.topClaimers.slice(0, 10).forEach((claimer, index) => {
              const percentage = stats.total > 0 ? Math.round((claimer.count / stats.total) * 100) : 0;
            %>
            <tr>
              <td>
                <% if (index === 0) { %>
                  <span style="font-size: 1.5rem;">ü•á</span>
                <% } else if (index === 1) { %>
                  <span style="font-size: 1.5rem;">ü•à</span>
                <% } else if (index === 2) { %>
                  <span style="font-size: 1.5rem;">ü•â</span>
                <% } else { %>
                  #<%= index + 1 %>
                <% } %>
              </td>
              <td><%= claimer.username || claimer.userId %></td>
              <td><strong><%= claimer.count %></strong></td>
              <td><span class="badge badge-success"><%= percentage %>%</span></td>
            </tr>
            <% }) %>
          </tbody>
        </table>
      </div>
    </div>
    <% } %>

    <!-- Recent Activity -->
    <div class="chart-container">
      <h2 class="chart-title">üìä Letzte 30 Tage</h2>
      <div class="stat-grid">
        <div class="stat-card">
          <span class="stat-icon">üìÖ</span>
          <h3>Heute</h3>
          <div class="stat-value"><%= stats.last30Days.today %></div>
        </div>
        <div class="stat-card">
          <span class="stat-icon">üìÜ</span>
          <h3>Letzte 7 Tage</h3>
          <div class="stat-value"><%= stats.last30Days.week %></div>
        </div>
        <div class="stat-card">
          <span class="stat-icon">üìà</span>
          <h3>Letzte 30 Tage</h3>
          <div class="stat-value"><%= stats.last30Days.month %></div>
        </div>
        <div class="stat-card">
          <span class="stat-icon">‚è±Ô∏è</span>
          <h3>√ò pro Tag</h3>
          <div class="stat-value"><%= stats.last30Days.avgPerDay %></div>
        </div>
      </div>
    </div>

    <!-- HEATMAP & INSIGHTS (Basic+ Feature) -->
    <% if (insights) { %>

    <!-- Hourly Activity Heatmap -->
    <div class="chart-container">
      <h2 class="chart-title">üïê Ticket-Erstellung nach Uhrzeit</h2>
      <p style="opacity: 0.7; margin-bottom: 1rem; font-size: 0.9rem;">
        Wann werden die meisten Tickets erstellt? (24-Stunden-Format)
      </p>
      <div class="bar-chart">
        <%
        const maxHourlyCount = Math.max(...insights.hourlyHeatmap) || 1;
        insights.hourlyHeatmap.forEach((count, hour) => {
          const percentage = (count / maxHourlyCount) * 100;
          const hourLabel = hour.toString().padStart(2, '0') + ':00';
        %>
        <div class="bar-item" style="margin-bottom: 0.5rem;">
          <div class="bar-label" style="min-width: 60px;"><%= hourLabel %></div>
          <div class="bar-container">
            <div class="bar-fill" style="width: <%= percentage %>%; background: linear-gradient(90deg, #0ea5e9, #0284c7);"><%= count %></div>
          </div>
        </div>
        <% }) %>
      </div>
    </div>

    <!-- Daily Activity Heatmap -->
    <div class="chart-container">
      <h2 class="chart-title">üìÖ Ticket-Erstellung nach Wochentag</h2>
      <p style="opacity: 0.7; margin-bottom: 1rem; font-size: 0.9rem;">
        An welchen Wochentagen kommen die meisten Tickets?
      </p>
      <div class="bar-chart">
        <%
        const dayNames = ['Sonntag', 'Montag', 'Dienstag', 'Mittwoch', 'Donnerstag', 'Freitag', 'Samstag'];
        const maxDailyCount = Math.max(...insights.dailyHeatmap) || 1;
        insights.dailyHeatmap.forEach((count, dayIndex) => {
          const percentage = (count / maxDailyCount) * 100;
        %>
        <div class="bar-item">
          <div class="bar-label" style="min-width: 100px;"><%= dayNames[dayIndex] %></div>
          <div class="bar-container">
            <div class="bar-fill" style="width: <%= percentage %>%; background: linear-gradient(90deg, #00ff88, #00dd77);"><%= count %></div>
          </div>
        </div>
        <% }) %>
      </div>
    </div>

    <!-- Topic Frequency Analysis -->
    <% if (insights.topicFrequency && insights.topicFrequency.length > 0) { %>
    <div class="chart-container">
      <h2 class="chart-title">üîù H√§ufigste Ticket-Kategorien</h2>
      <p style="opacity: 0.7; margin-bottom: 1rem; font-size: 0.9rem;">
        Welche Topics werden am h√§ufigsten genutzt?
      </p>
      <div class="bar-chart">
        <%
        const topTopics = insights.topicFrequency.slice(0, 10);
        const maxTopicFreq = topTopics[0]?.count || 1;
        topTopics.forEach((item) => {
          const percentage = (item.count / maxTopicFreq) * 100;
        %>
        <div class="bar-item">
          <div class="bar-label"><%= item.topic %></div>
          <div class="bar-container">
            <div class="bar-fill" style="width: <%= percentage %>%; background: linear-gradient(90deg, #764ba2, #667eea);"><%= item.count %></div>
          </div>
        </div>
        <% }) %>
      </div>
    </div>
    <% } %>

    <!-- Average Resolution Time by Topic -->
    <% if (insights.averageResolutionByTopic && insights.averageResolutionByTopic.length > 0) { %>
    <div class="chart-container">
      <h2 class="chart-title">‚è±Ô∏è Durchschnittliche L√∂sungszeit pro Kategorie</h2>
      <p style="opacity: 0.7; margin-bottom: 1rem; font-size: 0.9rem;">
        Wie schnell werden Tickets in verschiedenen Kategorien gel√∂st? (in Stunden)
      </p>
      <div class="table-container">
        <table>
          <thead>
            <tr>
              <th>Kategorie</th>
              <th>Gel√∂ste Tickets</th>
              <th>√ò Zeit</th>
              <th>Bewertung</th>
            </tr>
          </thead>
          <tbody>
            <% insights.averageResolutionByTopic.slice(0, 10).forEach((item) => {
              const hours = item.averageHours;
              const displayTime = hours < 1
                ? Math.round(hours * 60) + ' Min'
                : hours < 24
                ? Math.round(hours * 10) / 10 + ' Std'
                : Math.round(hours / 24 * 10) / 10 + ' Tage';

              // Rating based on resolution time
              let rating = 'üü¢ Schnell';
              if (hours > 24) rating = 'üî¥ Langsam';
              else if (hours > 6) rating = 'üü† Mittel';
            %>
            <tr>
              <td><strong><%= item.topic %></strong></td>
              <td><%= item.count %></td>
              <td><%= displayTime %></td>
              <td><%= rating %></td>
            </tr>
            <% }) %>
          </tbody>
        </table>
      </div>
    </div>
    <% } %>

    <!-- 30-Day Trend Analysis -->
    <% if (insights.trendAnalysis) { %>
    <div class="chart-container">
      <h2 class="chart-title">üìà Trend-Analyse (Letzte 30 Tage)</h2>
      <p style="opacity: 0.7; margin-bottom: 1rem; font-size: 0.9rem;">
        Entwicklung der Ticket-Erstellung √ºber die Zeit
      </p>
      <div class="bar-chart">
        <%
        const trendLabels = insights.trendAnalysis.labels;
        const trendCounts = insights.trendAnalysis.counts;
        const maxTrendCount = Math.max(...trendCounts) || 1;

        trendLabels.forEach((label, index) => {
          const count = trendCounts[index];
          const percentage = (count / maxTrendCount) * 100;
        %>
        <div class="bar-item" style="margin-bottom: 0.3rem;">
          <div class="bar-label" style="min-width: 70px; font-size: 0.85rem;"><%= label %></div>
          <div class="bar-container">
            <div class="bar-fill" style="width: <%= percentage %>%; background: linear-gradient(90deg, #10b981, #059669);"><%= count %></div>
          </div>
        </div>
        <% }) %>
      </div>
    </div>
    <% } %>

    <!-- Overall Insights Summary -->
    <div class="chart-container">
      <h2 class="chart-title">üí° Gesamtauswertung</h2>
      <div class="stat-grid">
        <div class="stat-card">
          <span class="stat-icon">üìä</span>
          <h3>Gesamt Tickets</h3>
          <div class="stat-value"><%= insights.totalTickets %></div>
        </div>
        <div class="stat-card">
          <span class="stat-icon">‚úÖ</span>
          <h3>Geschlossen</h3>
          <div class="stat-value"><%= insights.closedTickets %></div>
          <small style="opacity: 0.7;"><%= insights.totalTickets > 0 ? Math.round((insights.closedTickets / insights.totalTickets) * 100) : 0 %>%</small>
        </div>
        <div class="stat-card">
          <span class="stat-icon">üìÇ</span>
          <h3>Offen</h3>
          <div class="stat-value"><%= insights.openTickets %></div>
          <small style="opacity: 0.7;"><%= insights.totalTickets > 0 ? Math.round((insights.openTickets / insights.totalTickets) * 100) : 0 %>%</small>
        </div>
        <div class="stat-card">
          <span class="stat-icon">‚è±Ô∏è</span>
          <h3>√ò L√∂sungszeit</h3>
          <div class="stat-value">
            <%= insights.averageResolutionTimeHours < 1
              ? Math.round(insights.averageResolutionTimeHours * 60) + ' Min'
              : insights.averageResolutionTimeHours < 24
              ? Math.round(insights.averageResolutionTimeHours * 10) / 10 + ' Std'
              : Math.round(insights.averageResolutionTimeHours / 24 * 10) / 10 + ' Tage'
            %>
          </div>
        </div>
      </div>
    </div>

    <% } %>

  </div>

  <script>
    // Theme is managed by /js/theme-manager.js (Cookie-based)

    // Loading animation for back button
    const backBtn = document.querySelector('.back-btn');
    if (backBtn) {
      backBtn.addEventListener('click', function(e) {
        e.preventDefault();
        const href = this.getAttribute('href');

        const overlay = document.getElementById('loadingOverlay');
        const loadingText = document.getElementById('loadingText');

        loadingText.textContent = 'Zur√ºck zum Panel...';
        overlay.classList.add('show');

        setTimeout(() => {
          window.location.href = href;
        }, 100);
      });
    }
  </script>
  <script src="/js/auth-session.js"></script>
</body>
</html>
