  <!-- Delete Server Data Confirmation Modal -->
  <div class="modal-overlay" id="serverDeleteModal">
    <div class="modal-content">
      <div class="modal-icon">
        <i class="fas fa-exclamation-triangle"></i>
      </div>
      <h3 class="modal-title">Server-Daten l√∂schen?</h3>
      <p class="modal-message">
        M√∂chtest du alle Daten von <strong id="serverName"></strong> wirklich l√∂schen?
        <br><br>
        <span style="opacity: 0.7; font-size: 0.95rem;">
          Dies wird folgende Daten unwiderruflich l√∂schen:
          <br>‚Ä¢ Konfigurationsdateien
          <br>‚Ä¢ Alle Tickets
          <br>‚Ä¢ Ticket-Counter
          <br>‚Ä¢ Alle Transkripte
          <br><br>
          <strong style="color: #e74c3c;">Diese Aktion kann NICHT r√ºckg√§ngig gemacht werden!</strong>
        </span>
      </p>
      <div class="modal-buttons">
        <button class="modal-btn modal-btn-cancel" onclick="closeServerDeleteModal()">
          <i class="fas fa-times"></i> Abbrechen
        </button>
        <button class="modal-btn modal-btn-confirm" onclick="confirmServerDelete()">
          <i class="fas fa-trash"></i> Endg√ºltig l√∂schen
        </button>
      </div>
    </div>
  </div>

  <script>
    let currentDeleteFormId = null;
    let currentServerId = null;

    function copyToClipboard(text) {
      navigator.clipboard.writeText(text).then(() => {
        alert('Server ID kopiert: ' + text);
      });
    }

    const searchInput = document.getElementById('searchInput');
    const guildsTable = document.getElementById('guildsTable');
    const rows = Array.from(guildsTable.getElementsByTagName('tr'));

    searchInput.addEventListener('input', (e) => {
      const searchTerm = e.target.value.toLowerCase();

      rows.forEach(row => {
        const text = row.textContent.toLowerCase();
        if (text.includes(searchTerm)) {
          row.style.display = '';
        } else {
          row.style.display = 'none';
        }
      });
    });

    // Feedback Delete Modal Functions
    function showDeleteModal(feedbackId, feedbackName) {
      currentDeleteFormId = 'delete-form-' + feedbackId;
      document.getElementById('feedbackName').textContent = feedbackName;
      document.getElementById('deleteModal').classList.add('active');
    }

    function closeDeleteModal() {
      document.getElementById('deleteModal').classList.remove('active');
      currentDeleteFormId = null;
    }

    function confirmDelete() {
      if (currentDeleteFormId) {
        document.getElementById(currentDeleteFormId).submit();
      }
    }

    // Server Delete Modal Functions
    function showServerDeleteModal(serverId, serverName) {
      currentServerId = serverId;
      document.getElementById('serverName').textContent = serverName;
      document.getElementById('serverDeleteModal').classList.add('active');
    }

    function closeServerDeleteModal() {
      document.getElementById('serverDeleteModal').classList.remove('active');
      currentServerId = null;
    }

    function confirmServerDelete() {
      if (currentServerId) {
        // Create a form and submit it
        const form = document.createElement('form');
        form.method = 'POST';
        form.action = '/owner/delete-server-data/' + currentServerId;
        document.body.appendChild(form);
        form.submit();
      }
    }

    // Close modals on overlay click
    document.getElementById('deleteModal').addEventListener('click', function(e) {
      if (e.target === this) {
        closeDeleteModal();
      }
    });

    document.getElementById('serverDeleteModal').addEventListener('click', function(e) {
      if (e.target === this) {
        closeServerDeleteModal();
      }
    });

    // Close modals on Escape key
    document.addEventListener('keydown', function(e) {
      if (e.key === 'Escape') {
        closeDeleteModal();
        closeServerDeleteModal();
        closePremiumModal();
        closePremiumConfirm();
      }
    });

    // Premium Management Functions
    let currentPremiumServerId = null;
    let currentPremiumServerName = null;
    let pendingPremiumAction = null;

    function showPremiumModal(serverId, serverName, currentPremium) {
      currentPremiumServerId = serverId;
      currentPremiumServerName = serverName;
      document.getElementById('premiumServerName').textContent = serverName;
      document.getElementById('premiumServerId').textContent = serverId;

      // Set current premium badge
      const badgeClass = `badge badge-${currentPremium}`;
      document.getElementById('currentPremiumBadge').innerHTML = `<span class="${badgeClass}">${currentPremium}</span>`;

      document.getElementById('premiumModal').classList.add('active');
    }

    function closePremiumModal() {
      document.getElementById('premiumModal').classList.remove('active');
      currentPremiumServerId = null;
      currentPremiumServerName = null;
    }

    function showPremiumConfirm(type, tier) {
      if (!currentPremiumServerId) return;

      const days = document.getElementById('betaDays')?.value || 30;
      let icon = '';
      let iconColor = '';
      let title = '';
      let message = '';
      let buttonStyle = '';

      if (type === 'lifetime') {
        icon = 'fa-crown';
        iconColor = tier === 'pro' ? '#764ba2' : '#667eea';
        title = `Lifetime ${tier === 'pro' ? 'PRO' : 'Basic'} Premium aktivieren?`;
        message = `<strong>Unbegrenzter Zugang</strong> ohne Ablaufdatum f√ºr diesen Server.<br><br>` +
                  `‚úì ${tier === 'pro' ? 'Alle Pro-Features' : 'Alle Basic-Features'}<br>` +
                  `‚úì Keine monatlichen Kosten<br>` +
                  `‚úì Lebenslang g√ºltig`;
        buttonStyle = `background: linear-gradient(135deg, ${tier === 'pro' ? '#764ba2' : '#667eea'} 0%, ${tier === 'pro' ? '#667eea' : '#5a67d8'} 100%); color: #fff;`;
        pendingPremiumAction = { type: 'lifetime', tier, days: null };
      } else if (type === 'betatester') {
        icon = 'fa-flask';
        iconColor = 'var(--accent-color)';
        title = `Betatester f√ºr ${days} Tage aktivieren?`;
        message = `<strong>Pro-Level Features</strong> zum Testen f√ºr begrenzte Zeit.<br><br>` +
                  `‚úì Alle Pro-Features<br>` +
                  `‚úì L√§uft automatisch ab<br>` +
                  `‚úì Perfekt zum Testen`;
        buttonStyle = `background: linear-gradient(135deg, var(--accent-color) 0%, #00dd77 100%); color: #000; font-weight: 600;`;
        pendingPremiumAction = { type: 'betatester', tier: null, days };
      } else if (type === 'remove') {
        icon = 'fa-times-circle';
        iconColor = '#e74c3c';
        title = 'Premium wirklich entfernen?';
        message = `<strong style="color: #e74c3c;">Achtung:</strong> Alle Premium-Features werden deaktiviert!<br><br>` +
                  `‚úó Kategorien-Limit wird wiederhergestellt<br>` +
                  `‚úó Pro-Features werden deaktiviert<br>` +
                  `‚úó Diese Aktion ist sofort wirksam`;
        buttonStyle = `background: linear-gradient(135deg, #e74c3c 0%, #c0392b 100%); color: #fff;`;
        pendingPremiumAction = { type: 'remove', tier: null, days: null };
      }

      // Set icon and color
      const confirmIcon = document.getElementById('confirmIcon');
      confirmIcon.innerHTML = `<i class="fas ${icon}"></i>`;
      confirmIcon.style.color = iconColor;

      // Set content
      document.getElementById('confirmTitle').textContent = title;
      document.getElementById('confirmMessage').innerHTML = message;
      document.getElementById('confirmServerName').textContent = currentPremiumServerName;
      document.getElementById('confirmServerId').textContent = currentPremiumServerId;

      // Set button style
      const confirmButton = document.getElementById('confirmButton');
      confirmButton.style.cssText = buttonStyle + ' box-shadow: 0 4px 15px rgba(0, 0, 0, 0.3);';

      // Show confirmation modal
      document.getElementById('premiumConfirmModal').classList.add('active');
    }

    function closePremiumConfirm() {
      document.getElementById('premiumConfirmModal').classList.remove('active');
      pendingPremiumAction = null;
    }

    function confirmPremiumAction() {
      if (!pendingPremiumAction || !currentPremiumServerId) return;

      // Store action data before closing modal
      const actionData = {
        type: pendingPremiumAction.type,
        tier: pendingPremiumAction.tier,
        days: pendingPremiumAction.days,
        serverId: currentPremiumServerId
      };

      // Close confirm modal
      closePremiumConfirm();

      // Show loading
      showLoading('Premium wird aktualisiert...', 'Bitte warten');

      // Create form and submit
      const form = document.createElement('form');
      form.method = 'POST';
      form.action = '/owner/manage-premium';

      const serverIdInput = document.createElement('input');
      serverIdInput.type = 'hidden';
      serverIdInput.name = 'serverId';
      serverIdInput.value = actionData.serverId;
      form.appendChild(serverIdInput);

      const actionInput = document.createElement('input');
      actionInput.type = 'hidden';
      actionInput.name = 'action';
      actionInput.value = actionData.type;
      form.appendChild(actionInput);

      if (actionData.tier) {
        const tierInput = document.createElement('input');
        tierInput.type = 'hidden';
        tierInput.name = 'tier';
        tierInput.value = actionData.tier;
        form.appendChild(tierInput);
      }

      if (actionData.days) {
        const daysInput = document.createElement('input');
        daysInput.type = 'hidden';
        daysInput.name = 'days';
        daysInput.value = actionData.days;
        form.appendChild(daysInput);
      }

      document.body.appendChild(form);
      form.submit();
    }

    // Close premium modal on overlay click
    document.getElementById('premiumModal').addEventListener('click', function(e) {
      if (e.target === this) {
        closePremiumModal();
      }
    });

    document.getElementById('premiumConfirmModal').addEventListener('click', function(e) {
      if (e.target === this) {
        closePremiumConfirm();
      }
    });

    // Loading Animations
    function showLoading(text, subtext) {
      const overlay = document.getElementById('loadingOverlay');
      const loadingText = document.getElementById('loadingText');
      const loadingSubtext = overlay.querySelector('.loading-subtext');

      loadingText.textContent = text;
      if (subtext) {
        loadingSubtext.textContent = subtext;
      }
      overlay.classList.add('show');
    }

    // Add loading animation to home link
    const homeLink = document.querySelector('a[href="/"]');
    if (homeLink) {
      homeLink.addEventListener('click', function(e) {
        e.preventDefault();
        const href = this.getAttribute('href');
        showLoading('Zur√ºck zur Startseite...', 'Bitte warten');
        setTimeout(() => {
          window.location.href = href;
        }, 100);
      });
    }

    // Live Stats Auto-Refresh
    let statsUpdateInterval = null;
    let lastStats = {
      totalGuilds: null,
      totalTickets: null,
      botUptime: null,
      version: null
    };

    async function fetchLiveStats() {
      try {
        const response = await fetch('/api/owner/stats');
        if (!response.ok) throw new Error('Failed to fetch stats');

        const data = await response.json();
        if (data.success) {
          updateStatsDisplay(data.stats);
        }
      } catch (error) {
        console.error('Error fetching live stats:', error);
      }
    }

    function updateStatsDisplay(stats) {
      // Update each stat with smooth transition if value changed
      updateStatElement('stat-guilds', stats.totalGuilds, lastStats.totalGuilds);
      updateStatElement('stat-tickets', stats.totalTickets, lastStats.totalTickets);
      updateStatElement('stat-uptime', stats.botUptime, lastStats.botUptime);
      updateStatElement('stat-version', stats.version, lastStats.version);

      // Store current stats as last stats
      lastStats = { ...stats };
    }

    function updateStatElement(elementId, newValue, oldValue) {
      const element = document.getElementById(elementId);
      if (!element) return;

      // Only animate if value changed
      if (newValue !== oldValue && oldValue !== null) {
        element.style.transition = 'opacity 0.3s ease';
        element.style.opacity = '0.5';

        setTimeout(() => {
          element.textContent = newValue;
          element.style.opacity = '1';
        }, 300);
      } else if (oldValue === null) {
        // First load, no animation
        element.textContent = newValue;
      }
    }

    // Start auto-refresh when page loads
    window.addEventListener('DOMContentLoaded', () => {
      // Initial fetch
      fetchLiveStats();

      // Set up interval to fetch every 5 seconds
      statsUpdateInterval = setInterval(fetchLiveStats, 5000);
    });

    // Clean up interval when page unloads
    window.addEventListener('beforeunload', () => {
      if (statsUpdateInterval) {
        clearInterval(statsUpdateInterval);
      }
    });
  </script>
</body>
</html>
